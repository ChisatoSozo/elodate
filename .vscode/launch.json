  "version": "0.2.0",
  "configurations": [
    {
      "cwd": "client",
      "name": "Flutter for web",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome"
      ]
    },
    {
      "name": "client",
      "cwd": "client",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart"
    },
    {
      "name": "client (profile mode)",
      "cwd": "client",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "lib/main.dart"
    },
    {
      "name": "client (release mode)",
      "cwd": "client",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "lib/main.dart"
    }
  ]
}